{
  "name": "v2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass-chokidar": "^0.0.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.12",
    "redux": "^3.7.2",
